{
  "name": "parlor-booking-system",
  "version": "1.0.0",
  "description": "Beauty Parlor Booking System",
  "main": "backend/server.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/server.js",
